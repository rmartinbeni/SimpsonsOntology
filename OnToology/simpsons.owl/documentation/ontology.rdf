        <rdfs:domain rdf:resource="http://www.semanticweb.org/ricard/ontologies/2022/3/untitled-ontology-7#Location"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ricard/ontologies/2022/3/untitled-ontology-7#Episode"/>
        <rdfs:comment>Location appears on an episode</rdfs:comment>
        <rdfs:label>appearsOn</rdfs:label>
    </owl:ObjectProperty>
    


