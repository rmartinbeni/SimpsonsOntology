    <!-- http://www.semanticweb.org/ricard/ontologies/2022/3/untitled-ontology-7#appearsIn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ricard/ontologies/2022/3/untitled-ontology-7#appearsIn">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ricard/ontologies/2022/3/untitled-ontology-7#containsCharacter"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ricard/ontologies/2022/3/untitled-ontology-7#Character"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ricard/ontologies/2022/3/untitled-ontology-7#Episode"/>
        <rdfs:comment>A character appears on an episode</rdfs:comment>
        <rdfs:label>appearsIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ricard/ontologies/2022/3/untitled-ontology-7#appearsOn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ricard/ontologies/2022/3/untitled-ontology-7#appearsOn">
