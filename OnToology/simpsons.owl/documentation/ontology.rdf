        <rdfs:range rdf:resource="http://www.semanticweb.org/ricard/ontologies/2022/3/untitled-ontology-7#Family"/>
        <rdfs:comment>A character belongs to a family</rdfs:comment>
        <rdfs:label>belongsToFamily</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ricard/ontologies/2022/3/untitled-ontology-7#belongsToSeasson -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ricard/ontologies/2022/3/untitled-ontology-7#belongsToSeasson">
